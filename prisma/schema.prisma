generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DIRECT_URL")
}

model Account {
  id       Int      @id @default(autoincrement())
  name     String
  email    String   @unique
  data     Data[]
}

model Location {
  id        Int      @id @default(autoincrement())
  name      String
  latitude  Float
  longitude Float
  data      Data[]
}

model Data {
  id          Int       @id @default(autoincrement())
  timestamp   DateTime  @default(now())
  account     Account   @relation(fields: [accountId], references: [id])
  accountId   Int
  location    Location  @relation(fields: [locationId], references: [id])
  locationId  Int
  parameters  Json
  wqi         Json
}
